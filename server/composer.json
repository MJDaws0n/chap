{
    "name": "chap/server",
    "description": "Chap - Self-hosted deployment platform server",
    "version": "0.1.0",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chap\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "require": {
        "php": ">=8.1",
        "cboden/ratchet": "^0.4.4",
        "endroid/qr-code": "^5.0",
        "phpmailer/phpmailer": "^6.9",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "migrate": "php bin/migrate.php",
        "seed": "php bin/seed.php",
        "websocket": "php bin/websocket.php"
    }
}
